// Write a C program that accepts two integers from the user and calculates the product of the two integers.
#include <stdio.h>
int main()
{
    int num1, num2, product;
    printf("Num1=");
    scanf("%d", &num1);
    printf("Num2=");
    scanf("%d", &num2);

    product = num1 * num2;
    printf("Product=%d", product);

    return 0;
}